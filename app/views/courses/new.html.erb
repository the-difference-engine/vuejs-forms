<h1>Add a new course</h1>
<div id="new-course-form">
  <label for="name">Course Name:</label><input id="name" v-model="title">
  <label for="description">Course Description:</label><input id="description" v-model="description">
  <button v-on:click="saveForm(title, description)">Save Course</button>
  <div v-for="student in students">
    <label for="first_name">First Name</label>
    <input id="first_name" v-model="student.first_name">
    <label for="last_name">Last Name</label>
    <input id="last_name" v-model="student.last_name">
    <label for="email">Email</label>
    <input id="email" v-model="student.email">
  </div>
  <button v-on:click="addNewStudent">Add New Student</button>
</div>